{
  "name": "farmforecast",
  "version": "0.1.0",
  "description": "Agricultural intelligence platform for precision farming",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "npm run verify:credentials && echo 'Development server would start here'",
    "build": "npm run verify:credentials && echo 'Build would run here'",
    "test": "vitest",
    "test:unit": "vitest run --dir src/tests",
    "test:integration": "vitest run --dir src/tests/integration",
    "test:coverage": "vitest run --coverage",
    "verify:all": "node scripts/verify-credentials.js",
    "verify:neo4j": "node scripts/verify-credentials.js --service=neo4j",
    "verify:aws": "node scripts/verify-credentials.js --service=aws",
    "verify:weather": "node scripts/verify-credentials.js --service=weather",
    "verify:credentials": "node scripts/verify-credentials.js",
    "vault:encrypt": "dotenvx encrypt",
    "vault:decrypt": "dotenvx decrypt",
    "vault:setup": "node scripts/migrate-to-vault.js",
    "credentials:setup": "bash scripts/setup-credentials.sh",
    "credentials:rotate": "node scripts/rotate-credentials.js",
    "credentials:backup": "node scripts/backup-credentials.js",
    "onboard": "bash scripts/onboard-developer.sh",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "typecheck": "tsc --noEmit",
    "precommit": "npm run lint && npm run typecheck && npm run test:unit",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kswjdavis/FarmForecast.git"
  },
  "keywords": [
    "agriculture",
    "farming",
    "precision-agriculture",
    "neo4j",
    "graph-database",
    "weather-api",
    "crop-planning"
  ],
  "author": "American Agrionics",
  "license": "PROPRIETARY",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.600.0",
    "@aws-sdk/client-sts": "^3.600.0",
    "@aws-sdk/client-s3": "^3.600.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "@dotenvx/dotenvx": "^1.49.0",
    "neo4j-driver": "^5.24.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitest/coverage-v8": "^2.0.5",
    "detect-secrets": "^1.0.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "prettier": "^3.3.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vitest": "^2.0.5"
  },
  "volta": {
    "node": "20.15.1",
    "npm": "10.8.2"
  }
}